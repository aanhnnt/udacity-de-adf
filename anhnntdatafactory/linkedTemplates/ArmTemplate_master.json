{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "anhnntdatafactory"
		},
		"anhnntstorageaccount_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'anhnntstorageaccount'"
		},
		"anhnntsynapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'anhnntsynapse'"
		},
		"db_nycpayroll_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_nycpayroll'"
		},
		"anhnntstorageaccount_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://anhnntstorageaccount.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "anhnntdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"anhnntstorageaccount_accountKey": {
						"value": "[parameters('anhnntstorageaccount_accountKey')]"
					},
					"anhnntsynapse_connectionString": {
						"value": "[parameters('anhnntsynapse_connectionString')]"
					},
					"db_nycpayroll_connectionString": {
						"value": "[parameters('db_nycpayroll_connectionString')]"
					},
					"anhnntstorageaccount_properties_typeProperties_url": {
						"value": "[parameters('anhnntstorageaccount_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "anhnntdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'anhnntdatafactory_ArmTemplate_0')]"
			]
		}
	]
}